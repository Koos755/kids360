var form = $('<%= j(render 'form', authorization: @authorization, child: @child, organizations: @organizations) %>')
form.hide();
$('#click_to_add').before(form);
form.slideDown();
$('input#organization_name').typeahead({
  valueKey: "name",
  prefetch: '<%= organizations_url %>',
  limit: 5
});
$('#click_to_add').hide();
