var modal = $('<%= j(render 'modal', child: @child) %>');
$('#myModal').replaceWith(modal);
var form = $('<%= j(render 'authorizations/form', authorization: @authorization, child: @child, organizations: @organizations) %>')
form.hide();
$('#click_to_add').before(form);
form.slideDown();
$('input#organization_name').typeahead({
  valueKey: "name",
  prefetch: '<%= organizations_url %>',
  limit: 5
});
$('#myModal').modal();
$('#click_to_add').hide();
